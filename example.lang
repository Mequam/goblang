statement_end = \n
statement_group_start = (
statement_group_end = )
letter = [a-zA-Z]
digit = [0-9]
operator = +|-|*|/|d|a
statement = <expression><statement_end> | <statement_group_start><expression><statement_group_end>
number = <digit>+
name = <letter>+
range = [<number>-<number>]
equation =
				<equation><operator><equation>
				<number>
				<range>
assignment = let <name> = <value>
