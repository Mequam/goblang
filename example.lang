name = [a-zA-Z]+
const = [0-9]+
opperator = \+|-|/|\*
newline = \n
open_parenth = \(
closing_parenth = \)
equality = =

statement = <expr><newline>|<open_parenth><expr><closing_parenth>
expr = <assign>|<equation>
assign = <name><equality><equation>
equation =
				<const>
				<name>
				<equation><opperator><equation>|
